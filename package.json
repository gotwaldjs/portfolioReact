{
  "name": "jsg-portfolio",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://gotwaldjs.github.io/jsg-portfolio",
  "dependencies": {
    "@carbon/colors": "^11.14.0",
    "@carbon/elements": "^11.21.0",
    "@carbon/grid": "^11.13.0",
    "@carbon/icons": "^11.18.0",
    "@carbon/icons-react": "^11.18.0",
    "@carbon/layout": "^11.13.0",
    "@carbon/motion": "^11.10.0",
    "@carbon/pictograms": "^12.15.0",
    "@carbon/pictograms-react": "^11.41.0",
    "@carbon/react": "^1.27.0",
    "@carbon/styles": "^1.27.0",
    "@carbon/themes": "^11.18.0",
    "@carbon/type": "^11.17.0",
    "@storybook/react": "^7.0.5",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "gh-pages": "^5.0.0",
    "node": "^19.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.10.0",
    "react-scripts": "5.0.1",
    "sass": "^1.33.0",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start":"PUBLIC_URL=/jsg-portfolio react-scripts start",
    "predeploy": "yarn build",
    "deploy": "gh-pages -b master -d build",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "public",
          "css",
          "src"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^7.0.5",
    "@storybook/addon-essentials": "^7.0.5",
    "@storybook/addon-interactions": "^7.0.5",
    "@storybook/addon-links": "^7.0.5",
    "@storybook/addons": "^7.0.5",
    "@storybook/blocks": "^7.0.5",
    "@storybook/preset-create-react-app": "^7.0.5",
    "@storybook/react-webpack5": "^7.0.5",
    "@storybook/testing-library": "^0.0.14-next.2",
    "babel-plugin-named-exports-order": "^0.0.2",
    "prop-types": "^15.8.1",
    "storybook": "^7.0.5",
    "webpack": "^5.79.0"
  }
}
